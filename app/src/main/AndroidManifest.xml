            android:label="@string/title_activity_seal_cert_list">
        </activity>

        <receiver android:name=".NetworkStatusReceiver">
            <intent-filter>
                <action android:name="android.net.conn.CONNECTIVITY_CHANGE"/>
            </intent-filter>
        </receiver>

        <activity android:name=".ATestActivity">
        </activity>
        <activity android:name=".BarScanWarrantsActivity"
                  android:label="@string/title_activity_search">
