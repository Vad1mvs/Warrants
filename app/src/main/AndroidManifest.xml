        <activity
            android:name=".EmpWorkTimeActivity"
            android:label="@string/title_activity_emp_work_time">
        </activity>
        <activity
            android:name=".TrafficStatActivity"
            android:label="@string/title_activity_traffic_stat">
        </activity>
        <activity
            android:name=".SpecsSelectActivity"
            android:label="@string/title_activity_specs_select">
        </activity>
        <activity
            android:name=".location.GLocationActivity"
            android:label="@string/title_activity_glocation">
        </activity>
        <activity
            android:name=".EmpPhotoSelectActivity"
            android:label="@string/title_activity_emp_photo_select">
        </activity>

        <provider
            android:name=".WarrantsContentProvider"
            android:authorities="com.utis.warrants.dbprovider"/>

        <activity
            android:name=".SealViewActivity"
            android:label="@string/title_activity_seal_view">
        </activity>
        <activity
            android:name=".SealCertEditActivity"
            android:label="@string/title_activity_seal_cert_edit"
            android:windowSoftInputMode="stateHidden">
