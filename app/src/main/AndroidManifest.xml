<?xml version="1.0" encoding="utf-8"?>
<manifest package="com.utis.warrants"
          xmlns:android="http://schemas.android.com/apk/res/android"
          android:versionCode="58"
          android:versionName="1.58">

    <uses-sdk
        android:minSdkVersion="10"
        android:targetSdkVersion="15"/>

    <uses-feature
        android:glEsVersion="0x00020000"
        android:required="true"/>
    <uses-feature android:name="android.hardware.camera"/>

    <permission
        android:name="com.utis.warrants.permission.MAPS_RECEIVE"
        android:protectionLevel="signature"/>

    <uses-permission android:name="android.permission.CALL_PHONE"/>
    <uses-permission android:name="com.utis.warrants.permission.MAPS_RECEIVE"/>
    <uses-permission android:name="android.permission.ACCESS_NETWORK_STATE"/>
    <uses-permission android:name="android.permission.ACCESS_WIFI_STATE"/>
    <uses-permission android:name="android.permission.INTERNET"/>
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE"/>
    <uses-permission android:name="com.google.android.providers.gsf.permission.READ_GSERVICES"/>
    <!--
         The following two permissions are not required to use
		     Google Maps Android API v2, but are recommended.
    -->
    <uses-permission android:name="android.permission.ACCESS_COARSE_LOCATION"/>
    <uses-permission android:name="android.permission.ACCESS_FINE_LOCATION"/>
    <uses-permission android:name="android.permission.READ_PHONE_STATE"/>
    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE"/>

    <application
        android:icon="@drawable/utis_logo_64"
        android:label="@string/app_name"
        android:theme="@style/AppTheme">

        <!-- <uses-library android:name="com.google.android.maps"/> -->

        <activity
            android:name=".WelcomeActivity"
            android:label="@string/title_activity_welcome">
            <intent-filter>
                <action android:name="android.intent.action.MAIN"/>

                <category android:name="android.intent.category.LAUNCHER"/>
            </intent-filter>
        </activity>
        <activity
            android:name=".WebKitActivity"
            android:label="@string/title_activity_web_kit">
        </activity>
        <activity
            android:name=".WarrListActivity"
            android:label="@string/title_activity_warr_list">
        </activity>
        <activity
            android:name=".LoginActivity"
            android:label="@string/title_activity_login"
            android:windowSoftInputMode="adjustResize|stateVisible">
        </activity>
        <activity
            android:name=".WarrantsActivity"
            android:label="@string/title_activity_warrants"
            android:windowSoftInputMode="stateHidden">
        </activity>
        <activity
            android:name=".WarrContActivity"
            android:label="@string/title_activity_warr_cont"
            android:windowSoftInputMode="stateHidden">
        </activity>
        <activity
            android:name=".WarrUnitActivity"
            android:label="@string/title_activity_warr_unit">
        </activity>
        <activity
            android:name=".WarrRepairActivity"
            android:label="@string/title_activity_warr_repair">
        </activity>
        <activity
            android:name=".WarrantsMapActivity"
            android:label="@string/title_activity_map">
        </activity>
        <activity
            android:name=".JobGroupSelectActivity"
            android:label="@string/title_activity_job_group_select"
            android:windowSoftInputMode="stateHidden">
        </activity>
        <activity
            android:name=".JobSelectActivity"
            android:label="@string/title_activity_job_select"
            android:windowSoftInputMode="stateHidden">
        </activity>
        <activity
            android:name=".EmpHWSelectActivity"
            android:label="@string/title_activity_emp_hwselect">
        </activity>
        <activity
            android:name=".MessagesActivity"
            android:label="@string/title_activity_messages"
            android:windowSoftInputMode="stateHidden">
        </activity>
        <activity
            android:name=".MessageActivity"
            android:label="@string/title_activity_message">
        </activity>
        <activity
            android:name=".ComposeNewMsgActivity"
            android:label="@string/title_activity_compose_new_msg"
            android:windowSoftInputMode="adjustResize|stateVisible">
        </activity>
        <activity
            android:name=".EmpSelectActivity"
            android:label="@string/title_activity_emp_select">
        </activity>
        <activity
            android:name=".EmpMessagesActivity"
            android:label="@string/title_activity_user_messages">
        </activity>
        <activity
            android:name=".BarScanActivity"
            android:label="@string/title_activity_bar_scan">
        </activity>
        <activity
            android:name=".location.LocationActivity"
            android:label="@string/title_activity_location">
        </activity>

        <receiver android:name=".location.MyLocationReceiver"/>

        <service android:name=".location.MyGLocationService"/>
        <service android:name=".ConnectionManagerService"/>

        <activity
            android:name=".LogsActivity"
            android:label="@string/title_activity_log">
        </activity>
        <activity
            android:name=".ConnectSettingsActivity"
            android:label="@string/title_activity_connect_settings">
        </activity>
        <activity
            android:name=".WarrContSettingsActivity"
            android:label="@string/title_activity_warrcont_settings">
        </activity>

